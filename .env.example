# Copy to .env and fill in values.

# Frontend (optional)
VITE_GEMINI_API_KEY=

# Django
SECRET_KEY=
DEBUG=True

# Database (optional; project currently uses SQLite by default)
DB_NAME=
DB_USER=
DB_PASS=
